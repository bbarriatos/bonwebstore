<h2>{{ collections['frontpage'].title }}</h2>

{% for product in collections['frontpage'].products %}
    <div>
        <a href="{{ product.url }}">
            <img src="{{ product.featured_image | img_url: '480x480' }}" width="" height="" alt="">
            <h3>{{- product.title || upcase }}</h3>
            <p>{{ product.price | money }}</p>
        </a>
    </div>
{% endfor %}